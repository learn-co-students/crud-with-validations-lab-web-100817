<h1>Songs</h1>
<table style="width:100%">
  <th>Title</th>
  <th>Released</th>
  <th>Release Year</th>
  <th>Artist Name</th>
  <th>Genre</th>
<tbody>
  <% @songs.each do |song| %>
<tr>
  <td><%= song.title %></td>
  <td><%= song.released %></td>
  <td><%= song.release_year %></td>
  <td><%= song.artist_name %></td>
  <td><%= song.genre %></td>
  </tr>
<% end %>
</tbody>
</table>
